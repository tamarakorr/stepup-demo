    <div class="container">
      <!-- Example row of columns -->
      <div class="row">
        <div class="col-md-4">
          <!-- h2>Browse Vacancies</h2-->
          <%= button_to "Browse Vacancies", 'vacancies', :class => "btn btn-primary btn-lg", :method => :get %>
        </div>
        <div class="col-md-4">
          <!-- h2>Browse Volunteers</h2-->
          <%= button_to "Browse Volunteers", 'volunteers', :class => "btn btn-primary btn-lg", :method => :get %>
       </div>
        <div class="col-md-4">
          <!-- h2>More Info</h2-->
          <!-- p>Will display safety tips etc.</p-->
          <p><a class="btn btn-primary btn-lg" href="#" role="button">More Info</a></p>
        </div>
      </div>

      <!--hr-->

      <!--footer>
        <p>&copy; StepUp 2014</p>
      </footer-->
      <%= render 'layouts/footer' %>
    </div> <!-- /container -->


